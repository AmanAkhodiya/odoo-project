<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="cart_product_tree" model="ir.ui.view">
        <field name="name">cart.product.tree</field>
        <field name="model">cart.product</field>
        <field name="arch" type="xml">
            <tree>
                <field name='name' />
                <field name='price' />
                <field name='category'/>
                
                
            </tree>
        </field>
    </record>
    

    <record id="cart_action" model="ir.actions.act_window">
        <field name="name">cart</field>
        <field name="res_model">cart.product</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>